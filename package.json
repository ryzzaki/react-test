{
  "name": "react-test",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Cuong Nguyen",
  "license": "MIT",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "dev": "parcel src/index.html",
    "lint": "eslint \"src/**/*.{js, jsx}\" --quiet",
    "start": "yarn -s run build && babel-node server/index.js",
    "format": "prettier \"src/**/*.{js, html}\" --write"
  },
  "dependencies": {
    "@babel/node": "^7.7.7",
    "babel-cli": "^7.1.0",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.2.1",
    "express": "^4.17.1",
    "react": "15.x || 16.x || 16.4.0-alpha.0911da3",
    "react-dom": "15.x || 16.x || 16.4.0-alpha.0911da3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
